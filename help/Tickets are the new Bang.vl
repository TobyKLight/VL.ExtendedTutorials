              <p:stringtype p:Assembly="VL.Core" p:Type="VL.Core.StringType">Comment</p:stringtype>
            </p:ValueBoxSettings>
          </Pad>
          <Overlay Id="SPNfIDe7XwnNBAe6RuHUg5" Name="When do you need an alternative to bangs:" Bounds="220,595,556,984" />
          <Pad Id="QMZ8Wxzdcp7P0Dc2X9gM3v" Bounds="226,634,446,81" ShowValueBox="true" isIOBox="true" Value="&#xD;&#xA;This usually mean bangs become troublesome in two scenarios:&#xD;&#xA;A) from a stateless context&#xD;&#xA;EG You can't use TogEdge in a static op&#xD;&#xA;">
