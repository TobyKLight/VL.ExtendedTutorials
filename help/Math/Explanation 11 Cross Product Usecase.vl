                <p:OuterCategoryReference Kind="Category" Name="3D" NeedsToBeDirectParent="true" />
              </CategoryReference>
              <Choice Kind="OperationCallFlag" Name="Cross" />
            </p:NodeReference>
            <Pin Id="RyfgC3uIHH3QMDkv1Afdxb" Name="Input" Kind="StateInputPin" />
            <Pin Id="H6maCVJ76PeOq6QtqwcbUB" Name="Input 2" Kind="InputPin" />
